<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".LogInStudentSuccess">
    <AbsoluteLayout
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        tools:ignore="Deprecated,MissingConstraints"
        tools:layout_editor_absoluteX="109dp"
        tools:layout_editor_absoluteY="126dp">

        <TextView
            android:id="@+id/display"
            android:layout_width="354dp"
            android:layout_height="195dp"
            android:layout_x="30dp"
            android:layout_y="100dp"
            android:text="Log In Success screen for Student"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="Invalid"
            />

        <Button
            android:id="@+id/btn0"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_x="8dp"
            android:layout_y="532dp"
            android:text="@string/View_ALL_Courses"
            tools:ignore="MissingConstraints" />

        <Button
            android:id="@+id/btn1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_x="180dp"
            android:layout_y="573dp"
            android:text="@string/Search_By_Course_Name"
            tools:ignore="MissingConstraints" />

        <Button
            android:id="@+id/btn2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_x="9dp"
            android:layout_y="296dp"
            android:text="@string/Search_By_Course_Code"
            tools:ignore="MissingConstraints" />

        <Button
            android:id="@+id/btn3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_x="18dp"
            android:layout_y="616dp"
            android:text="@string/Search_By_Course_Day"
            tools:ignore="MissingConstraints" />

        <Button
            android:id="@+id/btn4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_x="271dp"
            android:layout_y="323dp"
            android:text="@string/Enroll"
            tools:ignore="MissingConstraints" />

        <Button
            android:id="@+id/btn5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_x="5dp"
            android:layout_y="340dp"
            android:text="@string/View_Enrolled_Courses"
            tools:ignore="MissingConstraints" />

        <Button
            android:id="@+id/btn6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_x="18dp"
            android:layout_y="572dp"
            android:text="@string/Drop_Course"
            tools:ignore="MissingConstraints" />

        <EditText
            android:id="@+id/ClassNameInput"
            android:layout_width="126dp"
            android:layout_height="wrap_content"
            android:layout_x="277dp"
            android:layout_y="453dp"
            android:ems="10"
            android:inputType="textPersonName" />

        <EditText
            android:id="@+id/ClassCodeInput"
            android:layout_width="131dp"
            android:layout_height="wrap_content"
            android:layout_x="274dp"
            android:layout_y="503dp"
            android:ems="10"
            android:inputType="textPersonName" />

        <TextView
            android:id="@+id/ClassName"
            android:layout_width="176dp"
            android:layout_height="39dp"
            android:layout_x="15dp"
            android:layout_y="452dp"
            android:text="Enter Class Name (or Class ID if changing ID):" />


        <EditText
            android:id="@+id/ClassDayInput"
            android:layout_width="126dp"
            android:layout_height="wrap_content"
            android:layout_x="276dp"
            android:layout_y="382dp"
            android:ems="10"
            android:inputType="textPersonName" />

        <TextView
            android:id="@+id/ClassCodeOrUsernameOrID"
            android:layout_width="283dp"
            android:layout_height="48dp"
            android:layout_x="0dp"
            android:layout_y="497dp"
            android:text="Enter Class Code or user ID or username (or Class Name if changing name):" />

        <TextView
            android:id="@+id/ClassDay"
            android:layout_width="283dp"
            android:layout_height="48dp"
            android:layout_x="4dp"
            android:layout_y="386dp"
            android:text="Enter A Class Day:" />

        <TextView
            android:id="@+id/display2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_x="22dp"
            android:layout_y="44dp"
            android:text="" />

    </AbsoluteLayout>

</androidx.constraintlayout.widget.ConstraintLayout>